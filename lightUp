public void lightUp(int x, int y, boolean horiz, boolean vert){
  for(int i=0;i<16;i++){
    if(vert)
    {
        Controller.mainGUI.ibutton.buttons[x][i].setIcon(Controller.mainGUI.buttonOnUp);
    }
    if(horiz)
    {
      Controller.mainGUI.ibutton.buttons[i][y].setIcon(Controller.mainGUI.buttonOnUp);
    }
  }
}
